
DOC: Invoc

InvocCount

map:

function (doc, meta) {
  if(doc.invocId)
    {
      emit(doc.invocId,null);
   } 
}

reduce:

_count



getLogEntriesForTaskOnHostSince

map:
function (doc, meta) {
  if(doc.hostname)
    {
  emit([doc.taskId, doc.hostname,doc.timestamp], null);
    }
    }
    
    
getHostNames

function (doc, meta) {
  if(doc.invocId)
    {
      emit(doc.hostname,doc.hostname);
   } 
}

map: _count


getLogEntriesForTasks

function (doc, meta) {
  if(doc.invocId)
    {
  emit(doc.taskId, null);
    }
}


getTaskname

function (doc, meta) {
  if(doc.taskId)
  {
    emit(doc.taskId,doc.taskname)
  }
}

